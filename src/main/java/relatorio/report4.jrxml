	<queryString language="SQL">
		<![CDATA[SELECT srcbd.marca.nome,
	srcbd.modelo.nome,
	srcbd.carro.id,
	srcbd.carro.cor,
	srcbd.carro.placa,
	srcbd.carro.chassi,
	srcbd.carro.ano,
	srcbd.carro.`IPVA`,
	srcbd.carro.seguro,
	srcbd.carro.garantia,
	srcbd.carro.`valorComprado`,
	srcbd.carro.`valorVenda`
FROM srcbd.carro
	INNER JOIN srcbd.modelo ON 
	 srcbd.carro.`id_Modelo` = srcbd.modelo.id 
	INNER JOIN srcbd.marca ON 
	 srcbd.modelo.`id_Marca` = srcbd.marca.id
WHERE $P{P_COD_MARCA} = srcbd.marca.id]]>
	</queryString>
